{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "typeRoots": ["typings", "node_modules/@types"],
        "baseUrl": "./",
        "paths": {
            "@components/*": ["src/components/*"],
            "@tokens/*": ["src/tokens/*"],
            "@typings/*": ["typings/*"],
            "@root/*": ["./*"]
        }
    }
}
